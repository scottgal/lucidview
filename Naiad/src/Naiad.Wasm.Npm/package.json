{
  "name": "@naiad/web-component-complete",
  "version": "0.1.0",
  "description": "Naiad complete WebAssembly web component with optional plugin packs enabled.",
  "type": "module",
  "main": "./dist/naiad-web-component.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./dist/naiad-web-component.js",
    "./client": "./dist/naiad-client.js",
    "./styles.css": "./dist/app.css"
  },
  "files": [
    "dist/_framework",
    "dist/naiad-web-component.js",
    "dist/naiad-client.js",
    "dist/app.css",
    "README.md",
    "index.d.ts"
  ],
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "prepack": "npm run build",
    "pack:preview": "npm pack --dry-run"
  },
  "keywords": [
    "mermaid",
    "wasm",
    "web-component",
    "naiad"
  ],
  "author": "mostlylucid",
  "license": "Unlicense",
  "sideEffects": true
}
